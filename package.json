{
  "name": "charcreat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "proxy": "http://localhost:5000",
    "start": "node backend/server.js",
    "dev": "concurrently \"vite --port 5173 --strictPort\" \"nodemon backend/server.js\"",
    "reseed-shop": "node backend/scripts/reseedShop.js",
    "reseed-weapons": "node backend/scripts/reseedWeapons.js",
    "reseed-all": "npm run reseed-shop && npm run reseed-weapons",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clear-shop": "node backend/scripts/clearShop.js",
    "clear-weapons": "node backend/scripts/clearWeapons.js",
    "reset-weapons": "npm run clear-weapons && npm run reseed-weapons",
    "reset-shop": "npm run clear-shop && npm run reseed-shop",
    "check-weapons": "node backend/scripts/checkWeapons.js"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.9",
    "@chakra-ui/theme-utils": "^2.0.21",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.17.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cannon-es": "^0.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-safe": "^9.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "framer-motion": "^12.16.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.554.0",
    "mongodb": "^6.9.0",
    "mongoose": "^8.9.4",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "openai": "^4.67.3",
    "react": "^18.3.1",
    "react-confetti": "^6.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "socket.io-client": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "three": "^0.171.0",
    "three-bmfont-text": "^2.3.0",
    "three-text2d": "^0.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.5.1",
    "concurrently": "^9.0.1",
    "eslint": "^9.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.10.0",
    "vite": "^6.3.5"
  }
}
